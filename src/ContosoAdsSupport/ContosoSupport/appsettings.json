{
  "AzureServicesAuthConnectionString": "RunAs=App",
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "ContosoSupport": "Debug"
    }
  },
  "AllowedHosts": "*",
  "Monitoring": {
    "Account": "ABCLab",
    "Namespace": "ContosoSupport",
    "Tenant": "<alias>",
    "ShutdownDurationSeconds": 30,
    "Behavior": {
      "BucketSize": 250,
      "BucketCount": 50,
      "MinimumValue": 10
    }
  },
  "Host": {
    "Urls": "http://*:5000"
  },
  "KeyVaultName": "ContosoAdsKeyVault"
}